procedure OnTick();
var
	GameTime: Cardinal;
	aHouses: array of Integer;
	h: Word;
	IronAmount, p: Byte;
		
	aScouts: array[0..2] of Integer;
begin
	GameTime := States.GameTime();
	
	//Schedule Events
	case GameTime of
		1: 	begin
				//Identify Mining-Town Storehouse
				aHouses := States.PlayerGetAllHouses(4);
				for h := 0 to Length(aHouses) -1 do
					if States.HouseType(aHouses[h]) = 11 then
						MiningVillageStorehouse := aHouses[h];
			end;
		//60: 	Actions.ShowMsg(-1, '<$0>'); //Initial Message
		150: begin
				Actions.ShowMsg(-1, '<$1>');
				//Change Alliance from Iron Mining Town...
				Actions.PlayerAllianceChange(0, 3, true, true);
				Actions.PlayerAllianceChange(1, 3, true, true);
				Actions.PlayerAllianceChange(2, 3, true, true);
				//...and Merchenary Town
				Actions.PlayerAllianceChange(0, 4, true, true);
				Actions.PlayerAllianceChange(1, 4, true, true);
				Actions.PlayerAllianceChange(2, 4, true, true);
				//Disable Fog Sharing
				Actions.PlayerShareFogCompliment(0, 3, false);
				Actions.PlayerShareFogCompliment(1, 3, false);
				Actions.PlayerShareFogCompliment(2, 3, false);
				Actions.PlayerShareFogCompliment(0, 4, false);
				Actions.PlayerShareFogCompliment(1, 4, false);
				Actions.PlayerShareFogCompliment(2, 4, false);
			end;
		750:	Actions.ShowMsg(-1, '<$2>');	
		900: begin
				//Give Scouts to Players (Don't want them to start bitching about no vision ;))
				aScouts[0] := Actions.GiveGroup(0, 21, 252, 109, 6, 4, 2);
				aScouts[1] := Actions.GiveGroup(1, 21, 254, 108, 6, 4, 2);
				aScouts[2] := Actions.GiveGroup(2, 21, 254, 111, 6, 4, 2);
				//Walk them to Player start locations
				Actions.GroupOrderWalk(aScouts[0], 186, 66, 4);
				Actions.GroupOrderWalk(aScouts[1], 154, 139, 4);
				Actions.GroupOrderWalk(aScouts[2], 174, 202, 4);
			end;
	end;
	
	//Transfer Iron to Players
	if GameTime mod 3000 = 0 then //Every 5 Minutes
	begin
		IronAmount := Utils.RoundTo((Utils.RandomRangeI(3, 15)), 3);
		if States.HouseResourceAmount(MiningVillageStorehouse, 3) >= IronAmount then
		begin
			for p := 0 to 2 do
			begin
				Actions.GiveWares(p, 3, IronAmount/3);
				if not bIronMsg then
					Actions.ShowMsg(p, '<$3>'); //Only show once
			end;
			bIronMsg := true;
			Actions.HouseTakeWaresFrom(MiningVillageStorehouse, 3, IronAmount);
		end;
	end;
end;

{
procedure OnPlayerDefeated(aPlayer: Integer);
var
	p: Byte;
begin
	if Difficulty = mdHard3 then
		if States.PlayerIsAI(aPlayer) then
			for p := 5 to 13 do
				if not States.PlayerDefeated(p) then
					//Increase Recruit Speed of not yet defeated players
					IronEquipRate(p, Round(States.AIEquipRate(p, 1) *0.945));
end;}